<h1>Create ya Employee</h1>
<hr>
<% if flash[:errors] %>
<% flash[:errors].each do |msg| %>
<%= msg %>
<br>
<% end%>
<% end%>
<br>
<%= form_for @employee do |f| %>
<%= f.label :first_name %>
<%= f.text_field :first_name %>
<br>
<%= f.label :last_name %>
<%= f.text_field :last_name %>
<br>
<%= f.label :alias %>
<%= f.text_field :alias %>
<br>
<%= f.label :title %>
<%= f.text_field :title %>
<br>
<%= f.label :office %>
<%= f.text_field :office %>
<br>
<%= f.label :img_url %>
<%= f.text_field :img_url %>
<br>
<%= collection_select(:employee, :dog_id, Dog.all, :id, :name, prompt: true) %>
<br>
<%= f.submit "Create" %>
<% end%>